{
    "name": "handcraftedinthealps/zendsearch",
    "description": "a general purpose text search engine written entirely in PHP 5",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "lucene"
    ],
    "homepage": "http://packages.zendframework.com/",
    "autoload": {
        "psr-4": {
            "ZendSearch\\": "library/ZendSearch"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "replace": {
        "zendframework/zendsearch": "self.version"
    }
}
